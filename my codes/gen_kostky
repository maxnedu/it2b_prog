
#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main() {
    int kostky, steny, cislo;
    printf("Zadej pocet kostek:");
    scanf("%d", &kostky);
    
    printf("Zadej pocet sten kostky:");
    scanf("%d", &steny);
    
    
    srand(time(NULL));
    printf("hody:");
    for(int i = 0; i < kostky; i++){
        cislo = rand() % steny + 1;
        printf(" %d", cislo);
        
    }
    
    
    



    return 0;
}